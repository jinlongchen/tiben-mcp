{
  "name": "@xiaozuzu/mcp-tiben-solver",
  "version": "1.0.17",
  "description": "MCP server that integrates with Tiben backend API to solve K-12 school problems",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-tiben-solver": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.cnb.cool/xiaozuzu_dev/xiaozuzu/-/packages/",
